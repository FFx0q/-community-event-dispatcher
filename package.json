{
  "name": "rpc-dispatcher",
  "version": "0.1.0",
  "description": "A simple event dispatcher which implements RPC pattern over RabbitMQ. Allows to register subscribers and dispatch events across the microservices.",
  "main": "lib/index.js",
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "standard src/*.js --fix",
    "prepare": "npm run lint && num run build",
    "dev": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FFx0q/rpc-dispatcher.git"
  },
  "keywords": [
    "amqp",
    "events",
    "rpc",
    "dispatcher"
  ],
  "author": "FFx0q",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.29.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^14.3.4"
  },
  "files": [
    "lib/**/*"
  ]
}
