  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.6"
  },
  "files": [
    "lib/**/*"
  ]
